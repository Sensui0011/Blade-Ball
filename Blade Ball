-- Create a ScreenGui and parent it to the PlayerGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Create a Frame for the GUI
local Frame = Instance.new("Frame")
Frame.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.224508882, 0, 0.0632653087, 0)
Frame.Size = UDim2.new(0, 68, 0, 77)
Frame.Active = true
Frame.Draggable = true

-- Create a UICorner for the Frame
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0.4, 0)
UICorner.Parent = Frame

-- Create the "Pc" TextButton
local TextButton = Instance.new("TextButton")
TextButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.176470593, 0, 0.111111112, 0)
TextButton.Size = UDim2.new(0, 44, 0, 26)
TextButton.Font = Enum.Font.FredokaOne
TextButton.Text = "Pc"
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton.TextSize = 14
TextButton.Parent = Frame

-- Create a UICorner for the "Pc" TextButton
local UICorner_2 = Instance.new("UICorner")
UICorner_2.CornerRadius = UDim.new(0.6, 0)
UICorner_2.Parent = TextButton

-- Create the "Cp" TextButton
local TextButton_2 = Instance.new("TextButton")
TextButton_2.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TextButton_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextButton_2.BorderSizePixel = 0
TextButton_2.Position = UDim2.new(0.176470593, 0, 0.548340619, 0)
TextButton_2.Size = UDim2.new(0, 44, 0, 26)
TextButton_2.Font = Enum.Font.FredokaOne
TextButton_2.Text = "Cp"
TextButton_2.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton_2.TextSize = 14
TextButton_2.Parent = Frame

-- Create a UICorner for the "Cp" TextButton
local UICorner_3 = Instance.new("UICorner")
UICorner_3.CornerRadius = UDim.new(0.6, 0)
UICorner_3.Parent = TextButton_2

-- Parent the Frame to the ScreenGui
Frame.Parent = ScreenGui
